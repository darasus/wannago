{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NODE_ENV",
    "MAILGUN_API_KEY",
    "UPSTASH_REDIS_REST_TOKEN",
    "UPSTASH_REDIS_REST_URL",
    "QSTASH_TOKEN",
    "NEXT_PUBLIC_GOOGLE_MAPS_API_KEY",
    "CLOUDFLARE_API_KEY"
  ],
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^generate", "^build"]
    },
    "lint": {
      "outputs": []
    },
    "generate": {
      "dependsOn": ["^generate"],
      "cache": false
    },
    "db:push": {
      "outputs": []
    },
    "test:e2e": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    }
  }
}
