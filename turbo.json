{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**"],
      "dependsOn": ["^generate"]
    },
    "lint": {
      "outputs": []
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "db:push": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
