{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NODE_ENV",
    "RESEND_API_KEY",
    "CLOUDFLARE_API_KEY",
    "CI",
    "VERCEL_ENV",
    "VERCEL_BRANCH_URL",
    "VERCEL_URL",
    "DATABASE_URL",
    "STRIPE_API_SECRET",
    "STRIPE_ENDPOINT_SECRET",
    "OAUTH_GOOGLE_CLIENT_ID",
    "OAUTH_GOOGLE_CLIENT_SECRET",
    "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",
    "NEXT_PUBLIC_GOOGLE_MAPS_API_KEY",
    "NEXT_PUBLIC_VERCEL_ENV",
    "NEXT_PUBLIC_VERCEL_URL",
    "NEXT_PUBLIC_VERCEL_BRANCH_URL",
    "NEXT_PUBLIC_BASE_URL"
  ],
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^generate", "^build"]
    },
    "lint": {
      "outputs": []
    },
    "generate": {
      "dependsOn": ["^generate"],
      "cache": false
    },
    "db:push": {
      "outputs": []
    },
    "test:e2e": {
      "outputs": [],
      "cache": false
    },
    "test:e2e:ci": {
      "outputs": [],
      "cache": false
    },
    "test:e2e:dev": {
      "outputs": [],
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^generate"]
    },
    "dev:inngest": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false
    }
  }
}
